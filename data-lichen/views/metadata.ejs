<!DOCTYPE html>
<html>
<head>
    <title>Data Lichen</title>
    <link rel="stylesheet" type="text/css" href="/styles.css">

</head>
<body>
    <h1>Data Lichen</h1>
    <table>
        <thead>
            <tr>
                <th>Service Name</th>
                <th>Service Address</th>
                <th>Completeness</th>
                <th>Validity</th>
                <th>Accuracy</th>
                <th>Processing Time</th>
                <th>Actual Time</th>
            </tr>
        </thead>
        <tbody>
            <% for(let item of metadata) { %>
                <tr>
                    <td><%= item.serviceName %></td>
                    <td><%= item.serviceAddress %></td>
                    <td><%= item.completeness %></td>
                    <td><%= item.validity %></td>
                    <td><%= item.accuracy %></td>
                    <td><%= item.processingTime %></td>
                    <td><%= item.actualTime %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>
